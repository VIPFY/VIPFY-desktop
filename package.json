{
  "name": "vipfy-desktop",
  "productName": "VIPFY",
  "version": "0.16.0",
  "description": "The desktop client for vipfy.com",
  "main": ".webpack/main",
  "scripts": {
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-win": "$env:LOCALAPPDATA = \"C:\\VIPFY\\vipfy-desktop\\tmp\" & electron-forge make",
    "make-mac-dev": "./prepare-build.sh true && ./build-mac-dev.sh",
    "make-mac-prod": "./prepare-build.sh && ./build-mac-prod.sh",
    "publish-js": "electron-forge publish",
    "publish-js-obfuscated": "jscrambler src/ssoConfigPreload/* -o obfuscated && electron-forge publish && shx rm -r obfuscated",
    "publish-js-obfuscated2": "electron-forge publish",
    "lint": "tslint --project tsconfig.json --type-check --force",
    "start": "electron-forge start",
    "faststart": "electron-forge start",
    "test": "env REACT_APP_TESTING=true TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha",
    "test-slow": "npm test -- -fgrep '@slow'",
    "test-fast": "npm run test-slow -- --invert",
    "test-sso": "npm test -- -fgrep 'SSO'",
    "test-sso-with-options": "env REACT_APP_TEST_SSO_WITH_OPTIONS=true npm run test-sso"
  },
