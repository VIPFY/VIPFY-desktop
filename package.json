{
  "name": "vipfy-desktop",
  "productName": "vipfy-desktop",
  "version": "0.3.1",
  "description": "The desktop client for vipfy.com",
  "main": "src/index.ts",
  "scripts": {
    "start-js": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish-js": "electron-forge publish",
    "lint": "tslint --project tsconfig.json --type-check --force",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "publish": "npm-run-all build-css build-js"
  },
  "keywords": [],
  "author": "Vipfy GmbH i.G.",
  "license": "proprietary",
  "prettier": {
    "printWidth": 100,
    "parser": "typescript",
    "jsxBracketSameLine": true
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "zip",
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "iconTransparent",
        "asar": true,
        "nucleus-uploader": {
          "host": "http://release.vipfy.com:7777",
          "appId": "1",
          "channelId": "acb2089ce426062c9a7430386ee57101",
          "token": "88632efd5e29a1424342a4c56ef25cd4"
        }
      },
      "publish_targets": {
        "darwin": [
          "nucleus-uploader"
        ],
        "win32": [
