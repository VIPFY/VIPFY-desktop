{
  "name": "vipfy-desktop",
  "productName": "VIPFY",
  "version": "0.4.20",
  "description": "The desktop client for vipfy.com",
  "main": "src/index.ts",
  "scripts": {
    "start-js": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-mac": "electron-forge package --platform=darwin && electron-forge make --platform=darwin",
    "publish-js": "electron-forge publish",
    "publish-mac": "electron-forge package --platform=darwin && electron-forge make --platform=darwin && electron-forge publish --platform=darwin",
    "lint": "tslint --project tsconfig.json --type-check --force",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "publish": "npm-run-all build-css build-js"
  },
  "keywords": [],
  "author": "Vipfy GmbH",
  "license": "proprietary",
  "prettier": {
    "printWidth": 100,
    "parser": "typescript",
    "jsxBracketSameLine": true
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "iconTransparent",
        "asar": true,
        "appCopyright": "Â©2018 VIPFY GmbH"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "vipfydesktop",
            "iconUrl": "https://vipfy.store/favicon.ico",
            "loadingGif": "./src/images/Ladeanimation_Vipfy_transparent.gif",
            "setupIcon": "./iconTransparent.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "linux",
            "darwin",
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
